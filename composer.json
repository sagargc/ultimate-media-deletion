{
    "name": "sagargc/ultimate-media-deletion",
    "description": "WordPress plugin for comprehensive media deletion",
    "version": "2.1.0",
    "type": "wordpress-plugin",
    "license": "GPL-3.0+",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "UltimateMediaDeletion\\": "includes/"
        }
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^2.3",
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "test": "phpunit --verbose",
        "test:debug": "phpunit --verbose --debug",
        "test:file": "phpunit --verbose tests/Unit/ExampleTest.php"
    }
}